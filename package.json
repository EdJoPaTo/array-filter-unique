{
	"name": "array-filter-unique",
	"version": "2.2.0",
	"description": "JavaScript array.filter() compatible unique filter",
	"license": "MIT",
	"repository": "EdJoPaTo/array-filter-unique",
	"author": {
		"name": "EdJoPaTo",
		"email": "array-filter-unique-npm-package@edjopato.de",
		"url": "https://edjopato.de"
	},
	"engines": {
		"node": ">=10"
	},
	"scripts": {
		"build": "del-cli dist && tsc",
		"prepack": "npm run build",
		"test": "tsc --sourceMap && xo && nyc ava"
	},
	"main": "dist/source",
	"types": "dist/source",
	"files": [
		"dist/source",
		"!*.test.*"
	],
	"keywords": [
		"array",
		"arr",
		"uniq",
		"unique",
		"duplicate"
	],
	"dependencies": {},
	"devDependencies": {
		"@sindresorhus/tsconfig": "^0.8.0",
		"@types/node": "^14.14.12",
		"ava": "^3.7.0",
		"del-cli": "^3.0.0",
		"nyc": "^15.0.1",
		"typescript": "^4.1.2",
		"xo": "^0.37.1"
	},
	"nyc": {
		"extension": [
			".ts"
		],
		"include": [
			"dist/source",
			"source"
		],
		"exclude": [
			"**/*.test.ts"
		],
		"all": true,
		"reporter": [
			"lcov",
			"text"
		]
	},
	"xo": {
		"rules": {
			"@typescript-eslint/prefer-readonly-parameter-types": "error",
			"ava/no-ignored-test-files": "off"
		},
		"overrides": [
			{
				"files": [
					"**/*.test.*",
					"test/**/*.*"
				],
				"rules": {
					"@typescript-eslint/prefer-readonly-parameter-types": "off"
				}
			}
		]
	}
}
